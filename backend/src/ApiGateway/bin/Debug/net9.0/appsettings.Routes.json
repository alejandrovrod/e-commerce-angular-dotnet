{
  "ReverseProxy": {
    "Routes": {
      "user-auth-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/api/auth/{**catch-all}" }
        ]
      },
      "user-addresses-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/addresses/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/api/addresses/{**catch-all}" }
        ]
      },
      "user-admin-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/admin/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "/api/admin/{**catch-all}" }
        ]
      },
      "user-profile-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/users/profile"
        },
        "Transforms": [
          { "PathPattern": "/api/users/profile" }
        ]
      },
      "user-update-profile-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/users/profile"
        },
        "Methods": ["PUT"],
        "Transforms": [
          { "PathPattern": "/api/users/profile" }
        ]
      },
      "user-password-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/users/password"
        },
        "Transforms": [
          { "PathPattern": "/api/users/password" }
        ]
      },
      "user-statistics-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/users/statistics"
        },
        "Transforms": [
          { "PathPattern": "/api/users/statistics" }
        ]
      },
      "user-export-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/api/admin/users/export"
        },
        "Transforms": [
          { "PathPattern": "/api/admin/users/export" }
        ]
      },
      "product-route": {
        "ClusterId": "product-cluster",
        "Match": {
          "Path": "/api/product/{**catch-all}"
        }
      },
      "category-route": {
        "ClusterId": "product-cluster",
        "Match": {
          "Path": "/api/categories/{**catch-all}"
        }
      },
      "order-route": {
        "ClusterId": "order-cluster",
        "Match": {
          "Path": "/api/orders/{**catch-all}"
        }
      },
      "payment-route": {
        "ClusterId": "payment-cluster",
        "Match": {
          "Path": "/api/payments/{**catch-all}"
        }
      },
      "file-route": {
        "ClusterId": "file-cluster",
        "Match": {
          "Path": "/api/files/{**catch-all}"
        }
      }
    }
  }
}
